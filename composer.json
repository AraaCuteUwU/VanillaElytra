{
  "name": "javierleon9966/vanillaelytra",
  "description": "A plugin that implements vanilla Elytra into PocketMine-MP",
  "type": "project",
  "minimum-stability": "dev",
  "license": "GPL-3.0-only",
  "require": {
    "pocketmine/pocketmine-mp": "^4.1.0",
    "blockhorizons/fireworks": "master"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.2.0",
    "phpstan/phpstan-strict-rules": "^1.0",
    "phpstan/extension-installer": "^1.0"
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "blockhorizons/fireworks",
        "version": "master",
        "autoload": {
          "classmap": ["src/"]
        },
        "source": {
          "url": "https://github.com/blockhorizons/fireworks",
          "type": "git",
          "reference": "4.0"
        }
      }
    }
  ],
  "autoload": {
    "psr-0": {
      "JavierLeon9966\\VanillaElytra\\": "src"
    }
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  }
}